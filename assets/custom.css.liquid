/* Position tooltip above the element */
span.data-tooltip {
    position: absolute;
    padding: 5px;
    -moz-border-radius: 3px;
    border-radius: 3px;
    background-color: #000;
    background-color: hsla(0, 0%, 20%, 0.9);
    color: #fff;
    font-size: 12px;
    line-height: 1;
    margin: auto;
    left: 0;
    right: 0;
      bottom: 0;
    pointer-events: none;
}
span.data-tooltip {
    visibility: hidden;
  opacity: 0;
  transition: visibility 0s, opacity 0.5s linear;
}
    
label.color_swatch:hover + span.data-tooltip {
    visibility: visible;
  opacity: 1;
}

